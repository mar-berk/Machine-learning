   ],
   "source": [
    "#To remove the mean of the id numbers we need to create a function that we then apply to the entire column\n",
    "def below_500(x):\n",
    "    if(x < 500.0): \n",
    "        return float('NaN')\n",
    "    else: \n",
    "        return x\n",
    "mean_steps = mean_steps[928].apply(below_500)\n",
    "mean_steps\n",
    "\n"
   ]
  },
